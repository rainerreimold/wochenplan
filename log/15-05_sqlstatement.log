replace into speisekomponente 
                set 
				bezeichnung 		= 'Bratkartoffeln',
				beschreibung 		= '<p>Kartoffeln roh in Scheiben geschnitten, in Schweineschmalz oder gew&uuml;rfeltem Speck angebraten, Salz, Pfeffer, Paprika, K&uuml;mmel und etwas Majoran.klein gew&uuml;rfelte Zwiebeln hinzugeben und goldgelb braten.</p>
'
